# - Config file for the FooBar package
# It defines the following variables
#  decnumber_INCLUDE_DIRS - include directories for FooBar
#  decnumber_LIBRARIES    - libraries to link against
 
# Compute paths
get_filename_component(DECNUMBER_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

set(decnumber_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

if(NOT TARGET decnumber AND NOT decnumber_BINARY_DIR)
  include("${DECNUMBER_CMAKE_DIR}/decnumberTargets.cmake")
endif()

set(decnumber_LIBRARIES decnumber)
