# - Config file for the FooBar package
# It defines the following variables
#  inteldfp_INCLUDE_DIRS - include directories for FooBar
#  inteldfp_LIBRARIES    - libraries to link against
 
# Compute paths
get_filename_component(DECNUMBER_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

set(inteldfp_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

if(NOT TARGET inteldfp AND NOT inteldfp_BINARY_DIR)
  include("${DECNUMBER_CMAKE_DIR}/inteldfpTargets.cmake")
endif()

set(inteldfp_LIBRARIES inteldfp)
