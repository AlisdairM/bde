# - Config file for the FooBar package
# It defines the following variables
#  pcre2_INCLUDE_DIRS - include directories for FooBar
#  pcre2_LIBRARIES    - libraries to link against
 
# Compute paths
get_filename_component(DECNUMBER_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

set(pcre2_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

if(NOT TARGET pcre2 AND NOT pcre2_BINARY_DIR)
  include("${DECNUMBER_CMAKE_DIR}/pcre2Targets.cmake")
endif()

set(pcre2_LIBRARIES pcre2)
